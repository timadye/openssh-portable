+			}
+		} else {
+			if (stage == 3) {
+				debug("variable '%.*s' to delete not found", lvar, var);
+			} else {
+				error("failed to get variable '%.*s' at stage %d", lvar, var, stage);
+			}
+		}
+	}
+
+	if (reg) RegCloseKey(reg);
+	if (user_root) RegCloseKey(user_root);
+	if (svar) free(svar);
+	if (var) free(var);
