+					if (full) debug("  -> '%.*s' = '%.*s'", lvar, var, lval, val);
+					else      debug("  -> '%.*s'",          lvar, var);
+					buffer_put_string(&msg, var, lvar);
+					if (full) buffer_put_string(&msg, val, lval);
+					nret++;
+				}
+			}
+		}
+	}
+	if (!success) {
+		error("failed to list variables at stage %d", stage);
+	}
