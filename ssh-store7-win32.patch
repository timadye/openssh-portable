+    if (argc >= 1) {
+      ret = ssh_delete_variable (agent_fd, argv[0], strlen(argv[0]), (delete==2));
+    } else {
+      ret = ssh_delete_variable (agent_fd, "",      0,               (delete==2));
+    }
+  }
